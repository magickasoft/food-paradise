{
  "name": "fit-paradise",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsc:check": "tsc --noEmit",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx,md}\"",
    "qlty:check": "qlty check --all",
    "qlty:fmt": "qlty fmt --all",
    "qlty:smells": "qlty smells --all",
    "biome:format": "./node_modules/.bin/biome format --write ./src",
    "biome:lint": "./node_modules/.bin/biome lint ./src",
    "biome:lint:fix": "./node_modules/.bin/biome lint --write ./src"
  },
  "dependencies": {
    "@artginzburg/next-ym": "^1.3.0",
    "next": "15.5.0",
    "next-intl": "^4.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@eslint/eslintrc": "^3",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "prettier": "^3.6.2",
    "typescript": "^5"
  },
  "trustedDependencies": [
    "unrs-resolver"
  ]
}
