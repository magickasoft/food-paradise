{
  "name": "food-paradise",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsc:check": "tsc --noEmit",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx,md}\"",
    "qlty:check": "qlty check --all",
    "qlty:fmt": "qlty fmt --all",
    "qlty:smells": "qlty smells --all",
    "biome:format": "./node_modules/.bin/biome format --write ./src",
    "biome:lint": "./node_modules/.bin/biome lint ./src",
    "biome:lint:fix": "./node_modules/.bin/biome lint --write ./src"
  },
  "dependencies": {
    "@artginzburg/next-ym": "^1.3.0",
    "next": "16.0.6",
    "next-intl": "4.5.5",
    "nodemailer": "^7.0.9",
    "nookies": "^2.5.2",
    "react": "19.2.0",
    "react-cookie": "^8.0.1",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@eslint/eslintrc": "^3",
    "@types/node": "^24",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "prettier": "^3.6.2",
    "typescript": "^5"
  },
  "trustedDependencies": [
    "unrs-resolver"
  ],
  "overrides": {
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3"
  }
